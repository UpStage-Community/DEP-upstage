<!DOCTYPE html>
<html>
<head>
    <title>Upstage</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
</head>
<body>
    <% flash.each do |key, value| %>
        <div>
            <%= value %>
        </div>
    <% end %>

    <div class="main-content">
        <%= yield %>
    </div>
    <%= render 'layouts/navigation' %>


</body>
</html>
